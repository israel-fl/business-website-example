<?php

use Illuminate\Database\Seeder;
use App\User;

class UserTableSeeder extends Seeder
{
    public function run()
    {
        User::create([
            'name'  => 'Admin',
            'email' => env('ADMIN_USER', ''),
            'password' => env('ADMIN_PASS', ''),
            'level' => 2,
            'verified' => true
            ]);
    }
}
